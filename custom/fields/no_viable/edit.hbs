				{{/each}}
			</select>
			<i class="icondefault icon icon-caret-down selectArrow-icon"></i>
		</div>
	</div>
	<!--fin Fuera de Perfil (Razón)-->

	<!-- Condiciones financieras-->
	<div class="field field--enum hide noViable condicionesFinancieras">
		<label class="field__label">Condiciones financieras</label>
		<div class="field__controls">
			<select name="enum" id="cond_financieras_leasing">
				{{#each razones_cf_list}}
				<option value="{{@key}}">{{this}}</option>
				{{/each}}
			</select>
			<i class="icondefault icon icon-caret-down selectArrow-icon"></i>
		</div>
	</div>
	<!--fin Condiciones financieras-->

	<!-- ¿Quién?-->
	<div class="field hide noViable competencia_quien">
		<label class="field__label">¿Quién?</label>
		<div class="field__controls">
			<span class="input-wrapper">
				<input type="text"  autocorrect="off" value="{{productos.leasing.competencia_quien}}" id="competencia_quien_leasing">
				<i class="icondefault icon icon-remove clear-button"></i>
			</span>
		</div>
	</div>

